{
  "name": "nbugs-web-demo",
  "version": "0.0.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "presite": "cd functions && npm install",
    "start": "cross-env PORT=8090 BROWSER=NONE NODE_ENV=test NODE_OPTIONS=--max_old_space_size=4096 COMPRESS=none APP_TYPE=site umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "deploy-gzip": "yarn && npm run build-test && ./gzip.sh test",
    "deploy-live-gzip": "yarn && npm run build-live && ./gzip.sh production",
    "deploy-pre-gzip": "yarn && npm run build-pre && ./gzip.sh pre",
    "build-pre": "cross-env BUILD_ENV=pre umi build",
    "cdn-pre": "nbugs-ship-cdn \"dist/**\" --base dist --env pre",
    "build-test": "cross-env BUILD_ENV=test umi build",
    "build-live": "cross-env BUILD_ENV=production umi build",
    "site": "npm run presite && cross-env APP_TYPE=site npm run build && firebase deploy && npm run docker:push",
    "analyze": "cross-env ANALYZE=1 NODE_OPTIONS=--max_old_space_size=4096 umi build",
    "lint:style": "stylelint 'src/**/*.less' --syntax less",
    "lint:prettier": "check-prettier lint",
    "lint": "eslint --ext .js src mock tests && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --ex .js src mock tests && stylelint --fix 'src/**/*.less' --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "tslint": "npm run tslint:fix",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'",
    "test": "umi test",
    "test:component": "umi test ./src/antd-pro-components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js",
    "deploy": "npm run build-test && npm run cdn",
    "deploy-live": "npm run build-live && npm run cdn-live",
    "cdn": "nbugs-ship-cdn \"dist/**\" --base dist",
    "cdn-live": "nbugs-ship-cdn \"dist/**\" --base dist --env production",
    "proxy": "node config/anyproxy.config.js",
    "pro": "node --max_old_space_size=9000 build-test/build-test.js",
    "fix-memory-limit": "cross-env LIMIT=8096 increase-memory-limit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.3.1",
    "@nbugs/nbugs-ship-cdn": "^2.3.0",
    "@umijs/plugin-qiankun": "^2.25.0",
    "@umijs/plugin-esbuild": "^1.3.1",
    "antd": "^3.13.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "increase-memory-limit": "^1.0.7",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.0",
    "moment": "^2.24.0",
    "nzh": "^1.0.4",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.6.2",
    "qiankun": "^2.4.2",
    "qs": "^6.6.0",
    "query-string": "^6.9.0",
    "react": "^16.7.0",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0",
    "react-media": "^1.9.2",
    "react-router-dom": "^4.3.1",
    "setprototypeof": "^1.1.1",
    "store2": "^2.7.1",
    "umi": "^3.4.24",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@umijs/preset-react": "^1.8.10",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.2.0",
    "anyproxy": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.4",
    "slash2": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-order": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0"
  },
  "optionalDependencies": {},
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=10.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
